<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="zenstruck_form.group_type.class">Zenstruck\Bundle\FormBundle\Form\Extension\GroupTypeExtension</parameter>
    </parameters>

    <services>
        <service id="zenstruck_form.group_type" class="%zenstruck_form.group_type.class%">
            <tag name="form.type_extension" extended_type="Symfony\Component\Form\Extension\Core\Type\FormType" />
        </service>
    </services>
</container>
