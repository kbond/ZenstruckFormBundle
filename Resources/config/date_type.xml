<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="zenstruck_form.date_type.class">Zenstruck\Bundle\FormBundle\Form\Type\DateType</parameter>
    </parameters>

    <services>
        <service id="zenstruck_form.date_type" class="%zenstruck_form.date_type.class%">
            <tag name="form.type" alias="zenstruck_date" />
        </service>
    </services>
</container>